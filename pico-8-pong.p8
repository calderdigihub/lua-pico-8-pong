pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- pico-8-pong
-- 
-- a pong implementation
-- for pico-8
--
-- made with <3 by
-- @pitto

-- scores
score1=0
score2=0

-- pad 1 variables
pad1w=4
pad1h=24
pad1x=0
pad1y=64-(pad1h/2)

-- pad 2 variables
pad2w=4
pad2h=24
pad2x=123
pad2y=64-(pad2h/2)

-- ball variables
ballx=64
bally=64
ballsize=3
ballxspeed=0
ballyspeed=0

function bounceball_debug()
    if btn(3) then
        ballyspeed=1
        ballxspeed=0
    elseif btn(2) then
        ballyspeed=-1
        ballxspeed=0
    elseif btn(1) then
        ballyspeed=0
        ballxspeed=1
    elseif btn(0) then
        ballyspeed=0
        ballxspeed=-1
    end
end

function movepaddle()
    if btn(2) and pad1y > 0 then
        pad1y-=4
    elseif btn(3) and pad1y + pad1h < 128 then
        pad1y+=4
    end
end

function moveball()
    ballx+=ballxspeed
    bally+=ballyspeed
end

function bounceball()
    -- top
    if bally < 0 then
        score1=bally
        ballyspeed=-ballyspeed
        sfx(0)
    elseif  bally + ballsize > 126 then
        score1=bally
        ballyspeed=-ballyspeed
    end
end

-- bounce the ball off the paddle
function bouncepaddle()
    print("ceppo")
end

function losedeadball()
    if bally>128 then
        sfx(3)
        bally=24
    end
end

function _update()
    movepaddle()
    moveball()
    bounceball()
    --bouncepaddle()
    --losedeadball()
    bounceball_debug()
end

function _draw()
    -- clear the screen
    rectfill(0,0, 128,128, 3)
    -- draw the 1st paddle
    rectfill(pad1x,pad1y, pad1x+pad1w,pad1y+pad1h, 15)
    -- draw the 2nd paddle
    rectfill(pad2x,pad2y, pad2x+pad2w,pad2y+pad2h, 15)
    -- draw the ball
    circfill(ballx,bally,ballsize,15)
    -- draw the score
    print(score1, 12, 6, 15)
end
